<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:state_enabled="true">
    <item >
        <bitmap
            android:src="@drawable/cross"
            android:gravity="center"
            />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/cross"/>
        <shape>
            <stroke android:width="2dp" android:color="#898989"/>
            <corners android:radius="7dp"/>
        </shape>

    </item>
</layer-list>

